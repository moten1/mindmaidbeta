{
  "name": "mindmaid-backend",
  "version": "2.0.1",
  "description": "MindMaid AI backend server powering emotional intelligence and mental wellness automation. Features caching, rate limiting, WebSocket streaming, and database integration.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"No test specified\" && exit 0",
    "logs": "type logs\\combined.log",
    "logs:error": "type logs\\error.log",
    "logs:watch": "powershell Get-Content logs\\combined.log -Wait -Tail 20",
    "logs:clear": "del /q logs\\*.log 2>nul || echo No logs to clear",
    "setup": "mkdir -p config logs && npm install",
    "clean": "rmdir /s /q logs node_modules || echo Cleaned",
    "health": "curl http://localhost:5000/api/health",
    "cache": "curl http://localhost:5000/api/cache/stats"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.9",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "mongoose": "^8.8.4",
    "morgan": "^1.10.0",
    "multer":"^1.4.4"",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.77.3",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moten1/mindmaidbeta.git"
  },
  "bugs": {
    "url": "https://github.com/moten1/mindmaidbeta/issues"
  },
  "homepage": "https://mindmaid.app",
  "keywords": [
    "ai",
    "mindmaid",
    "mental-health",
    "emotional-intelligence",
    "backend",
    "express",
    "hume",
    "openai",
    "google-ai",
    "mongodb",
    "render",
    "websocket",
    "node"
  ],
  "author": "Dr. Kashif Moten",
  "license": "MIT"
}
