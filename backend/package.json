{
  "name": "mindmaid-backend",
  "version": "2.0.0",
  "description": "MindMaid AI backend server with caching, rate limiting, logging, and database support",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "mkdir -p config logs && npm install",
    "clean": "rm -rf logs node_modules",
    "logs": "cat logs/combined.log",
    "logs:error": "cat logs/error.log",
    "health": "curl http://localhost:5000/api/health",
    "cache": "curl http://localhost:5000/api/cache/stats"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.9",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "mongoose": "^8.8.4",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.77.3",
    "pg": "^8.10.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "mindmaid",
    "backend",
    "express",
    "openai",
    "google-ai",
    "mongoose",
    "winston",
    "logging",
    "rate-limiting",
    "websocket"
  ],
  "author": "Kashif Moten",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/moten1/mindmaidbeta"
  },
  "bugs": {
    "url": "https://github.com/moten1/mindmaidbeta/issues"
  },
  "homepage": "https://mindmaid.app"
}
